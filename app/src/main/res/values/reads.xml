<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Key of the file to use to save the reads -->
    <string name="read_desc_save_file_name">%1$s_save.xml</string>

    <!-- Notification when a file fails to load/save -->
    <string name="read_desc_failure_load_file">Failed to load \"%1$s\" from local storage</string>
    <string name="read_desc_failure_save_file">Failed to save \"%1$s\" to local storage</string>

    <string name="read_desc_failure_remove_read">Failed to delete read \"%1$s\ from local storage"</string>

    <string name="read_desc_succeed_del_file">Successfully deleted corrupted read \"%1$s\"</string>
    <string name="read_desc_failure_del_file">Failed to delete corrupted read \"%1$s\"</string>

    <string name="read_desc_failure_thumbnail_load">Failed to load thumbnail \"%1$s\"</string>

    <string name="read_desc_failure_open_source">No application found to open \"%1$s\"</string>

    <string name="read_desc_failure_read_mode">Failed to load selected read</string>
    <string name="read_desc_success_read_mode">Saved progress for read \"%1$s\"</string>
    <string name="read_desc_failure_save_progress">Failed to save progression for \"%1$s\"</string>
    <string name="read_desc_success_save_progress">Reached %1$d%% completion for \"%2$s\"</string>

    <!-- Keys to register the read in a XML file -->
    <string name="read_desc_xml_key_root">root</string>
    <string name="read_desc_xml_key_uuid">uuid</string>
    <string name="read_desc_xml_key_name">name</string>
    <string name="read_desc_xml_key_type">type</string>
    <string name="read_desc_xml_key_source">source</string>
    <string name="read_desc_xml_key_creation_date">created_at</string>
    <string name="read_desc_xml_key_accessed_date">accessed_at</string>
    <string name="read_desc_xml_key_completion">completion</string>
    <string name="read_desc_xml_key_thumbnail">thumbnail</string>
</resources>